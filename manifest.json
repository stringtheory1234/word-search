{
	"manifest_version": 2,
	"name": "word search",
	"version": "1.0.0",
	"description": "searches words on net",
	"icons": {
		"128": "icon128.jpg"
	},
	"browser_action":{
		"default_icon": "icon16.jpg",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["p5.js", "p5.dom.js", "content.js"]
	}],
	"background":{
		"scripts": ["background.js"]
	}
}